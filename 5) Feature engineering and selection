encoders={}

for column in object_columns:
    label_encoder=LabelEncoder()
    df[column]=label_encoder.fit_transform(df[column])
    encoders[column]=label_encoder

with open("encoders.pkl","wb") as f:
    pickle.dump(encoders,f)

encoders


